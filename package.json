{
  "name": "test-micro",
  "version": "0.1.0",
  "private": true,
  "author": "DaYesahh",
  "scripts": {
    "all:start": "npm-run-all --parallel start:*",
    "all:install": "npm-run-all --serial install:*",
    "all:build": "npm-run-all --serial build:*",
    "install:main": "cd micro-main-app && npm install",
    "install:son1": "cd micro-son1-app && npm install",
    "build:main": "cd micro-main-app && npm run build",
    "build:son1": "cd micro-son1-app && npm run build",
    "start:main": "cd micro-main-app && npm run start",
    "start:son1": "cd micro-son1-app && npm run start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
